<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Rama.bloodshare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BloodShare">
        <activity android:name="com.Rama.bloodshare.Verify" />
        <activity android:name="com.Rama.bloodshare.AlmostThere" />
        <activity android:name="com.Rama.bloodshare.Register" />
        <activity android:name="com.Rama.bloodshare.Home" />
        <activity android:name="com.Rama.bloodshare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>